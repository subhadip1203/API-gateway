{
	"$schema": "https://www.krakend.io/schema/v3.json",
	"version": 3,
	"timeout": "3s",
	"cache_ttl": "300s",
	"port": 8080,
	"host": ["http://localhost:8000"],
	"endpoints": [
		{
			"endpoint": "/v1/test",
      "output_encoding":"json",
			"backend": [
				{
					"url_pattern": "/test",
          "encoding":"json"
				}
			]
		},
    {
			"endpoint": "/",
      "output_encoding":"json",
			"backend": [
				{
					"url_pattern": "/",
          "encoding":"json",
          "is_collection":true 
				}
			]
		}
	],
	"extra_config": {
    "telemetry/metrics": {
      "collection_time": "60s",
      "proxy_disabled": false,
      "router_disabled": false,
      "backend_disabled": false,
      "endpoint_disabled": false,
      "listen_address": ":8090"
    }
  }
}
